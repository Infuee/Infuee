* {
    margin: 0;
    padding: 0;
    box-sizing: border-box;
}

body {
    font-family: Montserrat;
}


/* == -- Scroll -- ==*/

body::-webkit-scrollbar {
    width: 10px;
    border-radius: 10px;
}


/* Handle */

body::-webkit-scrollbar-thumb {
    background: rgb(197, 197, 197);
    border-radius: 10px;
}

body::-webkit-scrollbar-thumb:hover {
    background: #999;
}


/* Track */

body::-webkit-scrollbar-track {
    background: #f1f1f1;
    border-radius: 10px;
}


/* -- //// -- */

li {
    list-style: none;
}

a {
    text-decoration: none;
}

.flex-center {
    display: flex;
