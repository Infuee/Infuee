/* -- Accept Proposal -- */

.accept-post-outer {
    background-color: #fff;
    width: 100%;
    max-width: 60%;
    margin: 0 auto;
    padding: 30px;
    border-radius: 4px;
}

.accept-post-head {
    display: flex;
    align-items: flex-start;
    margin-bottom: 20px;
}

.accept-post-head>img {
    width: 70px;
    margin-right: 30px;
    height: 70px;
}

.accept-post-head h2 {
    font-size: 40px;
    font-weight: 600;
    text-transform: uppercase;
}

.full-image {
    width: 100%;
}

.accept-post-content h4 {
    font-size: 16px;
    font-weight: 600;
    line-height: 22px;
    margin: 0 0 5px;
}

.accept-post-content p {
    color: #838383;
    font-size: 14px;
    font-weight: 500;
}

.accept-post-content img {
    width: 100%;
}

.post-button {
    /* max-width: 235px; */
    max-width: 100%;
    display: inline-block;
    text-align: center;
    background: #222;
    color: #fff;
    padding: 8px 10px;
    text-transform: uppercase;
    font-size: 14px;
    font-weight: 600;
    line-height: 20px;
    border: 2px solid #222;
    margin: 10px 0 0;
    transition: all 0.5s ease;
    border-radius: 4px;
}

.post-button:hover {
    background-color: transparent;
    color: #000;
}

.accept-post-footer {
    padding: 20px 0px 0;
}

.accept-post-footer h5 {
    font-size: 14px;
    font-weight: 600;
    line-height: 22px;
    color: #838383;
}

.link-text {
    width: 100%;
    max-width: 100%;
    display: inline-block;
    font-size: 18px;
    font-weight: 500;
    line-height: 30px;
    color: rgba(0, 0, 0, 0.6);
    border: 2px solid rgba(146, 146, 146, 0.8);
    padding: 10px 10px;
    margin: 10px 0;
    text-align: center;
    border-radius: 4px;
}

.link-text:focus {
    border: 2px solid #008ed7;
    outline: 0;
}

.link-text::placeholder {
    /* color: rgb(148, 148, 148); */
}

.highlight-funds {
    background: #000;
    color: #fff;
    font-size: 17px;
    font-weight: 600;
    line-height: 20px;
    border: 2px solid #000;
    text-align: center;
    margin: 10px 0 0;
    padding: 15px 20px;
    text-transform: uppercase;
    background-color: #008ed7;
    border: 2px solid #008ed7;
    border-radius: 4px;
    display: inline-block;
    transition: all 0.4s;
}

.highlight-funds:hover {
    color: #008ed7 !important;
    background-color: transparent !important;
    border: 2px solid #008ed7 !important;
    box-shadow: 0px 0px 10px rgb(0 142 215 / 45%) !important;
}

.noResult-img h3 {
    /* color: #fff; */
    font-weight: 700;
    letter-spacing: 1px;
    margin-top: 20px;
}

.swal2-container {
    z-index: 99999 !important;
}

.delete-notification {
    color: #fff;
    display: inline-block;
    padding: 2px;
    text-align: center;
    width: 22px;
    height: 22px;
    background-color: #0079bf;
    border-radius: 50%;
    font-size: 14px;
}
.delete-notification:hover {
    color: rgb(236, 236, 236);
}
@media only screen and (max-width: 1199px) {
    .accept-post-outer {
        max-width: 80%;
    }
}

@media only screen and (max-width: 991px) {
    .accept-post-outer {
        max-width: 100%;
    }
}

@media only screen and (max-width: 767px) {
    .accept-post-head>img {
        width: 95px;
    }
    .accept-post-head h2 {
        font-size: 35px;
    }
    .accept-post-content {
        margin-bottom: 20px;
    }
    .accept-post-footer {
        padding: 0;
    }
    .link-text {
        max-width: 100%;
    }
}

@media only screen and (max-width: 575px) {
    .accept-post-head>img {
        width: 80px;
        margin-right: 15px;
    }
    .accept-post-head h2 {
        font-size: 32px;
    }
    .post-button {
        margin-top: 0px;
        width: 100%;
        font-size: 12px;
    }
    .highlight-funds {
        width: 100%;
        font-size: 14px;
        padding: 10px 10px;
    }
    .link-text {
        font-size: 16px;
        line-height: 20px;
    }
    .accept-post-head {
        align-items: center;
    }
}

@media only screen and (max-width: 479px) {
    .accept-post-outer {
        padding: 15px;
    }
    .accept-post-head>img {
        width: 70px;
        margin-right: 10px;
    }
    .accept-post-head h2 {
        font-size: 23px;
    }
}

.required {
    color: red;
}

.message_count {
    display: none;
}
