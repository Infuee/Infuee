.header-main {
    position: sticky;
    top: 0%;
    z-index: 99999;
}

.header-main .header-wrapper nav.navbar ul.navbar-nav li {
    padding: 10px 25px;
}

.header-main .header-wrapper nav.navbar ul.navbar-nav li a {
    font-size: 14px;
    font-weight: 500;
    color: #fff;
    display: inline-block;
    padding: 0px;
    transition: all 0.3s;
}

.header-main .header-wrapper nav.navbar ul.navbar-nav li a.active {
    border-bottom: 1px solid #000;
    color: #000;
}

.header-main .header-wrapper nav.navbar ul.navbar-nav li a:hover {
    color: #000;
    border-bottom: 1px solid #000;
    transition: all 0.3s;
}

.header-main .header-wrapper nav.navbar .nav-right_btn .signup-btn,
.header-main .header-wrapper nav.navbar .nav-right_btn .login-btn {
    width: 110px;
    height: 40px;
    text-transform: uppercase;
    font-size: 14px;
    display: flex;
    align-items: center;
    justify-content: center;
}

.header-main .header-wrapper nav.navbar .nav-right_btn .signup-btn:focus,
.header-main .header-wrapper nav.navbar .nav-right_btn .login-btn:focus {
    outline: 0 !important;
    box-shadow: none !important;
}

.header-main .header-wrapper nav.navbar .nav-right_btn .signup-btn {
    background-color: #fff;
    border: 2px solid #fff;
    color: #008ed7;
    margin-left: 10px;
}

.header-main .header-wrapper nav.navbar .nav-right_btn .signup-btn:hover {
    background-color: #008ed7;
    color: #fff;
    border: 2px solid #fff;
}

.header-main .header-wrapper nav.navbar .nav-right_btn .login-btn {
    background-color: transparent;
    border: 2px solid #fff;
    color: #fff;
}

.header-main .header-wrapper nav.navbar .nav-right_btn .login-btn:hover {
    background-color: #fff;
    color: #008ed7;
}

.navbar.solid {
    background-color: #fff;
    transition: background-color 0.6s ease 0s;
    box-shadow: 0 0 4px grey;
}

.navbar.solid ul.navbar-nav li a {
    color: #000 !important;
}

.navbar.solid ul.navbar-nav li a.active {
    color: #008ed7 !important;
    border-bottom: 1px solid #008ed7 !important;
}

.navbar.solid ul.navbar-nav li a:hover {
    color: #008ed7 !important;
    border-bottom: 1px solid #008ed7 !important;
}

.navbar.solid .nav-right_btn .signup-btn {
    background-color: #008ed7 !important;
    border: 2px solid #008ed7 !important;
    color: #fff !important;
}

.navbar.solid .nav-right_btn .signup-btn:hover {
    background-color: transparent !important;
    color: #008ed7 !important;
}

.navbar.solid .nav-right_btn .login-btn {
    background-color: transparent !important;
    border: 2px solid #008ed7 !important;
    color: #008ed7 !important;
}

.navbar.solid .nav-right_btn .login-btn:hover {
    background-color: #008ed7 !important;
    color: #fff !important;
}

